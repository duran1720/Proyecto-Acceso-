generator client {
  provider = "prisma-client-js"
  
}

datasource db {
  provider = "mysql"
  url      = "mysql://root:1234@localhost:3306/accesodb"
}
model Usuario {
  id        Int      @id @default(autoincrement())
  nombre    String
  email     String   @unique
  password  String
  creadonEn DateTime @default(now())
}